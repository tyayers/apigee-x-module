{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch temptest.js",
      "program": "${workspaceFolder}/test/temptest.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "/home/tylerayers/key.json"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha All",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
          "-r",
          "ts-node/register",
          "--timeout",
          "999999",
          "--colors",
          "${workspaceFolder}/test/**/*.test.ts",
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "protocol": "inspector",
      "env":{
        "GOOGLE_APPLICATION_CREDENTIALS": "/home/tylerayers/key.json"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha proxy test",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
          "-r",
          "ts-node/register",
          "--timeout",
          "999999",
          "--colors",
          "${workspaceFolder}/test/proxies.test.ts",
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "protocol": "inspector",
      "env":{
        "GOOGLE_APPLICATION_CREDENTIALS": "/home/tylerayers/key.json"
      }
    },    
  ]
}